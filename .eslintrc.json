{
  "env": {
    "es2020": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      1,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "no-console": "off",
    "semi": "error"
  }
}
